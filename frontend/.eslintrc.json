{
  "env": {
    "browser": true,
    "es6": true,
    "es2017": true,
    "es2018": true,
    "es2020": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "react-app"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}