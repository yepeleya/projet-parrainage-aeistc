{
  "env": {
    "node": true,
    "es6": true,
    "es2017": true,
    "es2018": true,
    "es2020": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn"
  }
}